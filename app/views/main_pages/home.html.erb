    <section id="rsvp" class="rsvp">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <%= image_tag("img/rsvp.png", style: "width: 500px; height: 200px; margin-bottom: -50px; margin-left: 70px;") %>
                    <hr class="light">
                    <p style="text-align: center;">Please reply and indicate whether you will be able to attend</p>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
  <div class="col-md-8 col-md-offset-2 well" style="opacity: 0.7; background-color: #222222">
    <%= form_for(@detail, url: details_path) do |f| %>
    
    <div class="form-group">
      <div class="col-md-11">
        <%= f.label :name, class: "control-label" %>
        <%= f.text_field :name, class: "form-control", :required => 'required' %>
      </div>
    </div>
      
    <div class="form-group">
      <div class="col-md-11">
        <%= f.label :email, class: "control-label" %>
        <%= f.email_field :email, class: "form-control", :required => 'required' %>
      </div>
    </div>
      
    <div class="form-group">
      <div class="col-md-11">
        <%= f.label :phone, class: "control-label" %>
        <%= f.text_field :phone, class: "form-control", :required => 'required' %>
      </div>
    </div>
    
    <div class="form-group">
      <div class="col-md-4 control-label">
         <%= f.label :attend_ask, "Will You Attend" %>
      </div>
    </div>
    
     <div class="form-group">
      <div class="col-md-7">
        <%= f.radio_button(:attend, "Yes") %>
        <%= f.label :attend_yes, "Yes, I will be there", style: "color: white;" %><br>
        <%= f.radio_button(:attend, "No") %>
        <%= f.label :attend_no, "Sorry, I can't come", style: "color: white;" %>
      </div>
    </div>
    
    <div class="form-group">
      <div class="col-md-4 control-label">
         <%= f.label :batch, "SPM Batch "%>
      </div>
    </div>
    
    <div class="form-group">
      <div class="col-md-7">
        <%= f.select :batch, options_for_select([['Please select', nil], ['2008', 2008], ['2009', 2009], ['2010', 2010], ['2011', 2011], ['2012', 2012], ['2013', 2013], ['2014', 2014], ['2015', 2015], ['2016', 2016]], nil), {}, {class: "form-control", :required => 'required'} %>
      </div>
